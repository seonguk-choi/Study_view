	<script>//버거메뉴 클릭하면 .vp-wrap를 display: block 하는 명령
	const MOBILE = document.querySelector("#m_open");
	const VPWRAP = document.querySelector(".vp-wrap");
	
	
	MOBILE.addEvenlistener("click", function(){
		VPWRAP.style.display="block";
	});
	
